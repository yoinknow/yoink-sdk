{
  "name": "yoink-sdk",
  "version": "1.0.2",
  "description": "TypeScript SDK for Yoink bonding curve protocol on Solana - Buy, sell and query tokens with built-in slippage protection",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "browser": "./dist/browser/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "test": "node test-sdk.js",
    "test:tx": "node test-sdk-transactions.js",
    "build": "rimraf dist/ && npm run build:node && npm run build:browser && npm run copy:idl",
    "build:node": "tsc && tsc -p tsconfig.cjs.json",
    "build:browser": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "copy:idl": "cp src/IDL/yoink.json dist/cjs/IDL/ && cp src/IDL/yoink.json dist/esm/IDL/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "types",
    "assets",
    "README.md",
    "LICENSE",
    "QUICK_REFERENCE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoinknow/yoink-sdk.git"
  },
  "homepage": "https://yoink.now",
  "bugs": {
    "url": "https://github.com/yoinknow/yoink-sdk/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://yoink.now"
  },
  "social": {
    "twitter": "https://x.com/YoinkNow",
    "website": "https://yoink.now"
  },
  "keywords": [
    "yoink",
    "sdk",
    "typescript",
    "solana",
    "solana",
    "web3",
    "bonding-curve",
    "defi",
    "dex",
    "trading",
    "spl-token",
    "anchor"
  ],
  "author": {
    "name": "Yoink Team",
    "url": "https://yoink.now",
    "twitter": "https://x.com/YoinkNow"
  },
  "license": "ISC",
  "devDependencies": {
    "@coral-xyz/borsh": "^0.30.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.14.1",
    "dotenv": "^16.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^4.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "0.4.6",
    "@solana/web3.js": "^1.92.1",
    "bn.js": "^5.2.1"
  }
}
